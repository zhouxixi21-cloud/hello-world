# Generated by Ansible with network-scripts.j2
IPV6INIT=yes
BOOTPROTO=none
DEVICE={{ item.conn_name }}
ONBOOT=yes
TYPE=Ethernet
DEFROUTE=yes
IPV4_FAILURE_FATAL=no
NAME={{ item.conn_name }}
{% for dns in item.dns4 %}DNS{{ loop.index }}={{ dns }}
{% endfor %}
UUID={{ item.uuid }}
{% if item.ipaddr is defined %}IPADDR={{ item.ipaddr }}
{% endif %}
PREFIX=24
{% if item.gateway is defined %}GATEWAY={{ item.gateway }}
{% endif %}
{% if item.peerdns is defined %}PEERDNS={{ item.peerdns }}
{% endif %}
NM_CONTROLLED=no

